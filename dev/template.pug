include _assets/bemto/bemto
- set_bemto_settings({ modifier: "--" })

include Blocks/head/head
include Blocks/header/header-v1/header-v1
include Blocks/footer/footer

doctype html
html(lang="en")

	block head
		head
			+head({
				"title": "Arial gym",
				"author": "Denisoed",
				"description": "Arial gym"
			})
			
		+b.BODY.page(style="background-image: url('img/body-bg.jpg')")
			+e.DIV.wrap
				block header
					+header-v1()
					
				block content

				block footer
					+footer()
			block vendors
				// build:js js/vendor.js
				script( src="libs/jquery/dist/jquery.min.js")
				script( src="libs/owl.carousel/dist/owl.carousel.min.js")
				script( src="libs/jquery.mixitup/jquery.mixitup.min.js")
				script( src="libs/colorbox-master/jquery.colorbox-min.js")
				script( src="libs/jquery.sticky/jquery.sticky.js")
				// endbuild
				script( src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD-VAPLiH0Yb_wqRN7yiT_lvHljLQfNG8Q&callback=initMap" async defer )
					
			block scripts
				// build:js js/main.js
				script( src="js/main.js")
				// endbuild
